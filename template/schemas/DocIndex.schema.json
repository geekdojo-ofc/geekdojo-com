{
    "$schema": "https://dotnet.github.io/docfx/schemas/v1.0/schema.json#",
    "version": "1.0.0",
    "id": "https://github.com/dotnet/docfx/schemas/docindex.schema.json",
    "title": "DocIndex",
    "description": "The schema for a document index page",
    "type": "object",
    "xrefProperties": [ "title", "summary" ],
    "properties": {
        "metadata": {
            "type": "object",
            "tags": [ "metadata" ]
        },
        "uid": {
            "type": "string",
            "contentType": "uid"
        },
        "index": {
            "type": "object",
            "items": {
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string",
                        "contentType": "href"
                    },
                    "title": {
                        "type": "string"
                    }
                }
            }
        },
        "jsScripts": {
            "type": "array"
        }
    }
}