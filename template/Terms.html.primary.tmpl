{{!master(layout/_master.tmpl)}}
    <div class="profile-header"></div>
    <div class="header-logo-container"><div class="header-logo-block"></div></div>
    <div class="container-xxl">
        <header class="row profile-header-row">
            {{>partials/navbar}}
        </header>
        <section class="row is-card" style="background-color: var(--light-background) !important;">
            <div class="col-sm-12">
                <div class="columns is-masonry  non-uhf-padding" style="margin-top: 20px">
                    <!-- Termly Tracking Code -->
                    <div name="termly-embed" data-id="{{{termlyId}}}" data-type="iframe"></div>
                    <script type="text/javascript">(function(d, s, id) {
                    var js, tjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                    js = d.createElement(s); js.id = id;
                    js.src = "https://app.termly.io/embed-policy.min.js";
                    tjs.parentNode.insertBefore(js, tjs);
                    }(document, 'script', 'termly-jssdk'));</script>
                </div>
            </div>
        </section>
    </div>